{
  "name": "Usuario",
  "base": "User",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "properties": {
    "nombre": {
      "type": "string",
      "required": true
    },
    "apellidos": {
      "type": "string",
      "required": true
    }
  },
  "validations": [],
  "relations": {
    "comments": {
      "type": "hasMany",
      "model": "Comment",
      "foreignKey": "userId"
    },
    "quizzes": {
      "type": "hasMany",
      "model": "Quiz",
      "foreignKey": "userId"
    },
    "juegos": {
      "type": "hasMany",
      "model": "Juego",
      "foreignKey": "",
      "through": "Puntuacion"
    }
  },
  "acls": [
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "$everyone",
      "permission": "DENY"
    },
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "admin",
      "permission": "ALLOW"
    }
  ],
  "methods": {}
}
